import java.util.Scanner;

public class Additionmultiplication {
public static void main(String[] args)
{

int a,b,c,d,e,f;

System.out.println("Calculate the sum of two no.'s as addition:-");
Scanner sc = new Scanner(System.in);

a=sc.nextInt();
b=sc.nextInt();
c=a+b;


System.out.println("The sum of two no is :-"+c);

System.out.println("Calculate the sum of two no.'s as multiplication:-");
d=sc.nextInt();
e=sc.nextInt();
f=d*e;

System.out.println("the multiplication of two number is :-"+f);

}
}
